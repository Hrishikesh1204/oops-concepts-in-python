class Animal:
  def __init__(self, name):
    self.name = name
    print(f"{self.name} initialized")

  def speak(self):
    return f"{self.name} makes a sound"

  def eat(self):
    return f"{self.name} is eating"

class Dog(Animal):
  def __init__(self, name):
    super().__init__(name) 
   
    print(f"{self.name} is ready")

  def speak(self):
    return f"{self.name} says woof!"

