class Student:
  def __init__(self, name, roll_number):
    self.name = name
    self.roll_number = roll_number
student_hrishikesh = Student("Hrishikesh", "290")
print(f"The name of the student is: {student_hrishikesh.name}")
print(f"The roll number of the student is: {student_hrishikesh.roll_number}")
